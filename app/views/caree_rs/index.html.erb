<p id="notice"><%= notice %></p>

<h1>Careers</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th>Level education</th>
      <th>Modality</th>
      <th>Duration</th>
      <th>Working day</th>
      <th>Value semester</th>
      <th>Value curriculum</th>
      <th>Qualified registration</th>
      <th>Num credits</th>
      <th>Area knowledge</th>
      <th>Info additional</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @careers.each do |career| %>
      <tr>
        <td><%= career.ID %></td>
        <td><%= career.TITLE %></td>
        <td><%= career.LEVEL_EDUCATION %></td>
        <td><%= career.MODALITY %></td>
        <td><%= career.DURATION %></td>
        <td><%= career.WORKING_DAY %></td>
        <td><%= career.VALUE_SEMESTER %></td>
        <td><%= career.VALUE_CURRICULUM %></td>
        <td><%= career.QUALIFIED_REGISTRATION %></td>
        <td><%= career.NUM_CREDITS %></td>
        <td><%= career.AREA_KNOWLEDGE %></td>
        <td><%= career.INFO_ADDITIONAL %></td>
        <td><%= link_to 'Show', career %></td>
        <td><%= link_to 'Edit', edit_career_path(career) %></td>
        <td><%= link_to 'Destroy', career, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Career', new_career_path %>
